.tags {
  margin: 10rpx;
  white-space: nowrap;
  overflow-x: scroll;
}
.tag {
  display: inline-block;
  padding: 1rpx 10rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
  border-radius: 10rpx;
}
.tag.active {
  background-color: #4b8ccb;
  color: #fff;
}
.visualPage {
  font-size: 24rpx;
  display: flex;
  flex-wrap: wrap;
}
.visual-item {
  width: 33.3%;
}